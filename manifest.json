{
    "manifest_version": 3,
    "name": "Tab Saver",
    "version": "0.0.1",
    "description": "Save and restore browser tab sessions",
    "permissions": ["tabs", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "assets/icon_16.png",
      "48": "assets/icon_48.png",
      "128": "assets/icon_128.png"
    }
  }
  